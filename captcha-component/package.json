{
  "name": "captcha-component-accessibility",
  "description": "Captcha component for accessible places",
  "keywords": [
    "react",
    "react-ts",
    "typescript",
    "captcha",
    "accessibility",
    "accessible"
  ],
  "author": "Stack Attack Team",
  "private": false,
  "version": "0.3.0",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf lib",
    "copy:files": "copyfiles -e **/*.ts -e **/*.tsx -u 1 src/**/*.scss lib/esm/src",
